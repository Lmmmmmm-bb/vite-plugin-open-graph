{
  "name": "vite-plugin-open-graph",
  "type": "module",
  "version": "1.1.5",
  "packageManager": "pnpm@8.6.2",
  "description": "Generate open graph meta tags for your vite app.",
  "author": {
    "email": "lmmmmmm12138@gmail.com",
    "name": "_lmmmmmm",
    "url": "https://lmmmmmm.me"
  },
  "license": "MIT",
  "homepage": "https://github.com/Lmmmmmm-bb/vite-plugin-open-graph#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lmmmmmm-bb/vite-plugin-open-graph.git"
  },
  "bugs": "https://github.com/Lmmmmmm-bb/vite-plugin-open-graph/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "open-graph",
    "open-graph-protocol"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "vite playground",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "prepublishOnly": "unbuild",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@lmmmmmm/eslint-config": "^0.3.0",
    "@types/node": "^18.16.18",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vite": "^3.2.7",
    "vitest": "^0.32.2"
  }
}
